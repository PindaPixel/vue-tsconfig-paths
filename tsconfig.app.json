{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/app.tsbuildinfo",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*",
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.vue",
  ],
  "exclude": [
    "src/**/*.test.ts",
  ],
  "vueCompilerOptions": {
    "target": 3.5,
    "strictTemplates": true
  }
}